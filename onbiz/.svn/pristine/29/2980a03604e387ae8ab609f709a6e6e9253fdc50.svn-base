package onbiz.vo;

public class OnRiskSch {

  private String mem_no; //멤버번호 
  private String risk_name; // 리스크이름 
  private String risk_state; // 리스크상태 
  
  private int count; // 총데이터 건수
  private int pageSize; //한번에 보여줄 페이지 크기.
  private int pageCount; // 총 페이지 수 count/pageSize
  private int curPage; //클릭한 현재 페이지번호.
  private int start; //현재 페이지 시작 번호
  private int end; // 현재 페이지의 마지막 번호
  // 3. 블럭 처리 속성.
  private int blockSize; // 한번에 보여줄 block의 크기
  private int startBlock; // block의 시작번호
  private int endBlock; // block의 마지막번호
  public OnRiskSch() {
    super();
    // TODO Auto-generated constructor stub
  }
  public OnRiskSch(String mem_no, String risk_name, String risk_state, int count, int pageSize,
      int pageCount, int curPage, int start, int end, int blockSize, int startBlock, int endBlock) {
    super();
    this.mem_no = mem_no;
    this.risk_name = risk_name;
    this.risk_state = risk_state;
    this.count = count;
    this.pageSize = pageSize;
    this.pageCount = pageCount;
    this.curPage = curPage;
    this.start = start;
    this.end = end;
    this.blockSize = blockSize;
    this.startBlock = startBlock;
    this.endBlock = endBlock;
  }
  /**
   * @return the mem_no
   */
  public String getMem_no() {
    return mem_no;
  }
  /**
   * @param mem_no the mem_no to set
   */
  public void setMem_no(String mem_no) {
    this.mem_no = mem_no;
  }
  /**
   * @return the risk_name
   */
  public String getRisk_name() {
    return risk_name;
  }
  /**
   * @param risk_name the risk_name to set
   */
  public void setRisk_name(String risk_name) {
    this.risk_name = risk_name;
  }
  /**
   * @return the risk_state
   */
  public String getRisk_state() {
    return risk_state;
  }
  /**
   * @param risk_state the risk_state to set
   */
  public void setRisk_state(String risk_state) {
    this.risk_state = risk_state;
  }
  /**
   * @return the count
   */
  public int getCount() {
    return count;
  }
  /**
   * @param count the count to set
   */
  public void setCount(int count) {
    this.count = count;
  }
  /**
   * @return the pageSize
   */
  public int getPageSize() {
    return pageSize;
  }
  /**
   * @param pageSize the pageSize to set
   */
  public void setPageSize(int pageSize) {
    this.pageSize = pageSize;
  }
  /**
   * @return the pageCount
   */
  public int getPageCount() {
    return pageCount;
  }
  /**
   * @param pageCount the pageCount to set
   */
  public void setPageCount(int pageCount) {
    this.pageCount = pageCount;
  }
  /**
   * @return the curPage
   */
  public int getCurPage() {
    return curPage;
  }
  /**
   * @param curPage the curPage to set
   */
  public void setCurPage(int curPage) {
    this.curPage = curPage;
  }
  /**
   * @return the start
   */
  public int getStart() {
    return start;
  }
  /**
   * @param start the start to set
   */
  public void setStart(int start) {
    this.start = start;
  }
  /**
   * @return the end
   */
  public int getEnd() {
    return end;
  }
  /**
   * @param end the end to set
   */
  public void setEnd(int end) {
    this.end = end;
  }
  /**
   * @return the blockSize
   */
  public int getBlockSize() {
    return blockSize;
  }
  /**
   * @param blockSize the blockSize to set
   */
  public void setBlockSize(int blockSize) {
    this.blockSize = blockSize;
  }
  /**
   * @return the startBlock
   */
  public int getStartBlock() {
    return startBlock;
  }
  /**
   * @param startBlock the startBlock to set
   */
  public void setStartBlock(int startBlock) {
    this.startBlock = startBlock;
  }
  /**
   * @return the endBlock
   */
  public int getEndBlock() {
    return endBlock;
  }
  /**
   * @param endBlock the endBlock to set
   */
  public void setEndBlock(int endBlock) {
    this.endBlock = endBlock;
  }
}
