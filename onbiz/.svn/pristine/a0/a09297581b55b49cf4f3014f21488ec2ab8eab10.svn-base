package onbiz.vo;

import java.util.Date;


// 작업 OnTask 
// 작업번호(pk), 작업명, 작업내용, 작업등록일, 작업수정일, 작업시작일, 작업종료일, 작업 진행상태, 작업 우선순위, 상위작업번호, 프로젝트번호(fk), 작업 담당자(fk)
// task_no, task_name, task_content, task_regdte, task_uptdte, task_startdte, task_enddte, task_prior, taskref_no, mem_no
public class OnTask {
	String task_no;
	String task_name;
	String task_content;
	Date task_regdte;
	Date task_uptdte;
	Date task_startdte;
	Date task_enddte;
	String task_state;
	int task_prior;
	String taskref_no;
	String mem_no;
	
	public OnTask() {
		super();
	}

	public String getTask_no() {
		return task_no;
	}

	public void setTask_no(String task_no) {
		this.task_no = task_no;
	}

	public String getTask_name() {
		return task_name;
	}

	public void setTask_name(String task_name) {
		this.task_name = task_name;
	}

	public String getTask_content() {
		return task_content;
	}

	public void setTask_content(String task_content) {
		this.task_content = task_content;
	}

	public Date getTask_regdte() {
		return task_regdte;
	}

	public void setTask_regdte(Date task_regdte) {
		this.task_regdte = task_regdte;
	}

	public Date getTask_uptdte() {
		return task_uptdte;
	}

	public void setTask_uptdte(Date task_uptdte) {
		this.task_uptdte = task_uptdte;
	}

	public Date getTask_startdte() {
		return task_startdte;
	}

	public void setTask_startdte(Date task_startdte) {
		this.task_startdte = task_startdte;
	}

	public Date getTask_enddte() {
		return task_enddte;
	}

	public void setTask_enddte(Date task_enddte) {
		this.task_enddte = task_enddte;
	}

	public String getTask_state() {
		return task_state;
	}

	public void setTask_state(String task_state) {
		this.task_state = task_state;
	}

	public int getTask_prior() {
		return task_prior;
	}

	public void setTask_prior(int task_prior) {
		this.task_prior = task_prior;
	}

	public String getTaskref_no() {
		return taskref_no;
	}

	public void setTaskref_no(String taskref_no) {
		this.taskref_no = taskref_no;
	}

	public String getMem_no() {
		return mem_no;
	}

	public void setMem_no(String mem_no) {
		this.mem_no = mem_no;
	}

}
