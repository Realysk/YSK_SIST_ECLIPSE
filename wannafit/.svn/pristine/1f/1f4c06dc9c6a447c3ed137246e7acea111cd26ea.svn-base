package wannafit.content.vo;

// 회원 VO
// 주소 : wannafit.content.vo.User
public class User {
	// 필수정보
	String wm_id;			// 아이디
	String wm_pass;			// 비밀번호
	String wm_email;		// 이메일
	String wm_nick;			// 닉네임
	String wm_tel;				// 연락처
	String wm_usrGroup;		// 회원구분(관리자)
	int wm_repCnt;			// 신고횟수
	String wm_imgSrc; 		// 프로필이미지
	String wb_num; 			// 사업자번호
	
	// 선택정보 - 일반회원 (join)
	String wm_gender;		// 성별
	int wm_age;				// 연령대
	int wm_sports;			// 운동분야 유무
	String wm_result;		// 가입이유

	// 회원구분 
	String wm_usrGroup_Title;

	public User() {
		super();
	}
	
	// 아이디 중복체크
	public User(String wm_id) {
		super();
		this.wm_id = wm_id;
	}

	public User(String wm_id, String wm_pass) {
		super();
		this.wm_id = wm_id;
		this.wm_pass = wm_pass;
	}
	
	public User(String wm_id, String wm_pass, String wm_usrGroup_Title) {
		super();
		this.wm_id = wm_id;
		this.wm_pass = wm_pass;
		this.wm_usrGroup_Title = wm_usrGroup_Title;
	}
	
	// (성근) 회원정보 수정을 위해서 만들어둘께요!
	public User(String wm_id, String wm_pass, String wm_email, String wm_nick, String wm_tel) {
		super();
		this.wm_id = wm_id;
		this.wm_pass = wm_pass;
		this.wm_email = wm_email;
		this.wm_nick = wm_nick;
		this.wm_tel = wm_tel;
	}	
	
	// 로그인용
	public User(String wm_id, String wm_pass, String wm_email, String wm_nick, String wm_tel, String wm_usrGroup,
			int wm_repCnt, String wm_imgSrc, String wb_num) {
		super();
		this.wm_id = wm_id;
		this.wm_pass = wm_pass;
		this.wm_email = wm_email;
		this.wm_nick = wm_nick;
		this.wm_tel = wm_tel;
		this.wm_usrGroup = wm_usrGroup;
		this.wm_repCnt = wm_repCnt;
		this.wm_imgSrc = wm_imgSrc;
		this.wb_num = wb_num;
	}

	public User(String wm_id, String wm_pass, String wm_email, String wm_nick, String wm_tel, String wm_imgSrc,
			String wm_gender, int wm_age, int wm_sports, String wm_result) {
		super();
		this.wm_id = wm_id;
		this.wm_pass = wm_pass;
		this.wm_email = wm_email;
		this.wm_nick = wm_nick;
		this.wm_tel = wm_tel;
		this.wm_imgSrc = wm_imgSrc;
		this.wm_gender = wm_gender;
		this.wm_age = wm_age;
		this.wm_sports = wm_sports;
		this.wm_result = wm_result;
	}

	// 회원가입용
	public User(String wm_id, String wm_pass, String wm_email, String wm_nick, String wm_tel, String wm_usrGroup,
			int wm_repCnt, String wm_imgSrc, String wb_num, String wm_gender, int wm_age, int wm_sports,
			String wm_result) {
		super();
		this.wm_id = wm_id;
		this.wm_pass = wm_pass;
		this.wm_email = wm_email;
		this.wm_nick = wm_nick;
		this.wm_tel = wm_tel;
		this.wm_usrGroup = wm_usrGroup;
		this.wm_repCnt = wm_repCnt;
		this.wm_imgSrc = wm_imgSrc;
		this.wb_num = wb_num;
		this.wm_gender = wm_gender;
		this.wm_age = wm_age;
		this.wm_sports = wm_sports;
		this.wm_result = wm_result;
	}	

	public String getWm_id() {
		return wm_id;
	}



	public void setWm_id(String wm_id) {
		this.wm_id = wm_id;
	}

	public String getWm_pass() {
		return wm_pass;
	}

	public void setWm_pass(String wm_pass) {
		this.wm_pass = wm_pass;
	}

	public String getWm_email() {
		return wm_email;
	}

	public void setWm_email(String wm_email) {
		this.wm_email = wm_email;
	}

	public String getWm_nick() {
		return wm_nick;
	}

	public void setWm_nick(String wm_nick) {
		this.wm_nick = wm_nick;
	}

	public String getWm_tel() {
		return wm_tel;
	}

	public void setWm_tel(String wm_tel) {
		this.wm_tel = wm_tel;
	}

	public String getWm_usrGroup() {
		return wm_usrGroup;
	}

	public void setWm_usrGroup(String wm_usrGroup) {
		this.wm_usrGroup = wm_usrGroup;
	}

	public int getWm_repCnt() {
		return wm_repCnt;
	}

	public void setWm_repCnt(int wm_repCnt) {
		this.wm_repCnt = wm_repCnt;
	}

	public String getWm_imgSrc() {
		return wm_imgSrc;
	}

	public void setWm_imgSrc(String wm_imgSrc) {
		this.wm_imgSrc = wm_imgSrc;
	}

	public String getWb_num() {
		return wb_num;
	}

	public void setWb_num(String wb_num) {
		this.wb_num = wb_num;
	}

	public String getWm_gender() {
		return wm_gender;
	}

	public void setWm_gender(String wm_gender) {
		this.wm_gender = wm_gender;
	}

	public int getWm_age() {
		return wm_age;
	}

	public void setWm_age(int wm_age) {
		this.wm_age = wm_age;
	}

	public int getWm_sports() {
		return wm_sports;
	}

	public void setWm_sports(int wm_sports) {
		this.wm_sports = wm_sports;
	}

	public String getWm_result() {
		return wm_result;
	}

	public void setWm_result(String wm_result) {
		this.wm_result = wm_result;
	}

	public String getWm_usrGroup_Title() {
		return wm_usrGroup_Title;
	}

	public void setWm_usrGroup_Title(String wm_usrGroup_Title) {
		this.wm_usrGroup_Title = wm_usrGroup_Title;
	}
}
